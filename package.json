{
  "name": "zeelib",
  "description": "Node utility library",
  "version": "11.2.0",
  "author": {
    "email": "zac@zacanger.com",
    "name": "Zac Anger",
    "url": "http://zacanger.com"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/zacanger"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "if [ -d lib ] ; then rm -r lib/ ; fi && mkdir -p lib/",
    "cover": "nyc npm run test:tape",
    "doc": "typedoc --theme minimal --out docs/ src",
    "postpublish": "npm run doc && git commit -am 'doc [skip ci]' && git push origin master",
    "prebuild": "run-s -s sortpack clean test:build",
    "preversion": "npm run build",
    "sortpack": "sortpack",
    "test": "run-s -s test:ts test:lint test:tape",
    "test:build": "run-s -s test:lint test:ts cover",
    "test:lint": "ts-standard src",
    "test:tape": "ts-node node_modules/.bin/tape src/*.test.ts | tap-spec",
    "test:ts": "tsc --noEmit"
  },
  "homepage": "https://zacanger.github.io/zeelib",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/zeelib.git"
  },
  "bugs": "https://github.com/zacanger/zeelib/issues",
  "keywords": [
    "lib",
    "library",
    "micro-library",
    "node",
    "util",
    "utility"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/node": "14.14.20",
    "@types/tape": "4.13.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "sortpack": "2.1.11",
    "tap-spec": "5.0.0",
    "tape": "5.1.1",
    "ts-node": "9.1.1",
    "typedoc": "0.20.13",
    "typescript": "4.1.3"
  },
  "engines": {
    "node": ">=21.0.0"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/*.ts"
    ],
    "instrument": true,
    "reporter": [
      "text",
      "html",
      "json"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true
  }
}
