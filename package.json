{
  "name": "zeelib",
  "description": "Util lib",
  "version": "11.2.0",
  "author": {
    "email": "zac@zacanger.com",
    "name": "Zac Anger",
    "url": "http://zacanger.com"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/zacanger"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "if [ -d lib ] ; then rm -r lib/ ; fi && mkdir -p lib/",
    "doc": "typedoc --includeVersion --out docs/ src/*.ts --exclude '**/*+(index|.test).ts'",
    "postpublish": "npm run doc && git commit -am 'doc [skip ci]' && git push origin master",
    "prebuild": "run-s -s sortpack clean test:build",
    "preversion": "npm run build",
    "sortpack": "sortpack",
    "test": "run-s -s test:ts test:lint test:node",
    "test:build": "run-s -s test:lint test:ts cover",
    "test:lint": "eslint -c .eslintrc.js src/*.ts --fix",
    "test:node": "node --test -r ts-node/register src/*.test.ts",
    "_test:node": "node --test --test-reporter=tap --experimental-test-coverage -r ts-node/register src/",
    "test:ts": "tsc --noEmit"
  },
  "homepage": "https://zacanger.github.io/zeelib",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/zeelib.git"
  },
  "bugs": "https://github.com/zacanger/zeelib/issues",
  "keywords": [
    "lib",
    "library",
    "micro-library",
    "node",
    "util",
    "utility"
  ],
  "devDependencies": {
    "@types/node": "20.10.6",
    "eslint": "8.56.0",
    "eslint-config-standard-with-typescript": "43.0.0",
    "npm-run-all": "4.1.5",
    "sortpack": "2.3.5",
    "ts-node": "10.9.2",
    "typedoc": "0.25.4",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=21.0.0"
  }
}
