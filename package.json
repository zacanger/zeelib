{
  "name": "zeelib",
  "description": "A small lib of FP, Node, and DOM utils.",
  "version": "8.4.0",
  "author": {
    "name": "Zac Anger",
    "email": "zac@zacanger.com",
    "url": "http://zacanger.com"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel --ignore \"src/*.test.js\" src/ -d lib/",
    "clean": "if [ -d lib ] ; then rm -r lib/ ; fi && mkdir -p lib/",
    "copy-flow": "flow-copy-source -i \"*.test.js\" src lib",
    "cover": "run-s -s cover:tape cover:flow",
    "cover:flow": "flow-coverage-report --config .flow-coverage-report.json",
    "cover:tape": "nyc npm run test:tape",
    "doc": "documentation build -g src/index.js -f html -o docs",
    "fixpack": "fixpack",
    "postbuild": "npm run copy-flow",
    "postpublish": "npm run doc && git commit -am 'doc [skip ci]' && git push",
    "prebuild": "run-s -s clean fixpack test:build",
    "preversion": "npm run build",
    "test": "run-s -s test:flow test:lint test:tape",
    "test:build": "run-s -s test:lint test:flow cover:flow cover:tape",
    "test:flow": "flow check",
    "test:lint": "eslint -c .eslintrc.json src",
    "test:tape": "babel-tape-runner src/*.test.js | tap-spec",
    "update-types": "flow-typed update-cache && install --overwrite"
  },
  "homepage": "https://zacanger.github.io/zeelib",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/zeelib.git"
  },
  "bugs": "https://github.com/zacanger/zeelib/issues",
  "keywords": [
    "dom",
    "fp",
    "lib",
    "library",
    "node",
    "utils"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.5",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-3": "6.24.1",
    "babel-tape-runner": "2.0.1",
    "documentation": "8.0.0",
    "eslint": "5.11.1",
    "eslint-config-zacanger": "3.1.4",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-flowtype-errors": "3.6.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-unicorn": "4.0.3",
    "fixpack": "2.3.1",
    "flow-bin": "0.75.0",
    "flow-copy-source": "2.0.2",
    "flow-coverage-report": "0.6.1",
    "flow-typed": "2.5.1",
    "npm-run-all": "4.1.5",
    "nyc": "12.0.2",
    "tap-spec": "5.0.0",
    "tape": "4.9.2"
  },
  "files": [
    "lib",
    "warning.js"
  ],
  "nyc": {
    "include": [
      "src"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": true,
    "instrument": true,
    "reporter": [
      "text",
      "html",
      "json"
    ]
  }
}
