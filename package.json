{
  "name": "zeelib",
  "description": "Node utility library",
  "version": "11.0.8",
  "author": {
    "email": "zac@zacanger.com",
    "name": "Zac Anger",
    "url": "http://zacanger.com"
  },
  "license": "LGPL-3.0",
  "main": "lib/index.js",
  "files": [
    "lib",
    "warning.js"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "if [ -d lib ] ; then rm -r lib/ ; fi && mkdir -p lib/",
    "cover": "nyc npm run test:tape",
    "doc": "typedoc --mode file --module commonjs --theme minimal --out docs/ src",
    "format": "sortpack && prettier --write \"src/*.ts\"",
    "postpublish": "npm run doc && git commit -am 'doc [skip ci]' && git push origin master",
    "prebuild": "run-s -s sortpack clean test:build",
    "preversion": "npm run build",
    "sortpack": "sortpack",
    "test": "run-s -s test:ts test:lint test:tape",
    "test:build": "run-s -s test:lint test:ts cover",
    "test:lint": "eslint --ext .ts -c .eslintrc.json src",
    "test:tape": "ts-node node_modules/.bin/tape src/*.test.ts | tap-spec",
    "test:ts": "tsc --noEmit"
  },
  "homepage": "https://zacanger.github.io/zeelib",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/zeelib.git"
  },
  "bugs": "https://github.com/zacanger/zeelib/issues",
  "keywords": [
    "lib",
    "library",
    "micro-library",
    "node",
    "util",
    "utility"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/node": "13.1.8",
    "@types/tape": "4.2.33",
    "eslint": "6.8.0",
    "eslint-plugin-zacanger": "0.0.3",
    "npm-run-all": "4.1.5",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "sortpack": "2.0.4",
    "tap-spec": "5.0.0",
    "tape": "4.13.0",
    "ts-node": "8.6.2",
    "typedoc": "0.16.7",
    "typescript": "3.7.5"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/*.ts"
    ],
    "instrument": true,
    "reporter": [
      "text",
      "html",
      "json"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/zacanger"
  }
}
