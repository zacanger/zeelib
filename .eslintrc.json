{
  "root": true,
  "plugins": [
    "jane"
  ],
  "extends": [
    "plugin:jane/recommended",
    "plugin:jane/typescript",
    "plugin:jane/prettier-ts",
    "plugin:jane/jest"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "env": {
    "node": true
  },
  "rules": {
    "no-console": 0,
    "@typescript-eslint/no-explicit-any": 0
  }
}
